<medicalappointment-detail mode="vm.mode"
                           model="vm.model"
                           loading="vm.loading"
                           on-update="vm.updateMedicalAppointment(event)"
                           on-confirm="vm.confirmMedicalAppointment(event)"
                           on-cancel="vm.cancelMedicalAppointment(event)"
                           on-delete="vm.deleteMedicalAppointment(event)"
                           on-reload="vm.load()">
</medicalappointment-detail>