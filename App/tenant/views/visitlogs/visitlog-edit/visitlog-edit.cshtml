<visitlog-detail mode="vm.mode"
                 model="vm.model"
                 loading="vm.loading"
                 on-update="vm.updateVisitLog(event)"
                 on-confirm="vm.confirmVisitLog(event)"
                 on-cancel="vm.cancelVisitLog(event)"
                 on-delete="vm.deleteVisitLog(event)"
                 on-reload ="vm.load()">
</visitlog-detail>