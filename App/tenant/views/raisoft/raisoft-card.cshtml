<div id="Raisoft" class="portlet light" busy-if="$ctrl.loading" ng-class="{ 'div-disabled': $ctrl.mode=='new' }">
    <div class="portlet-title">
        <div class="caption">
            <span class="fontStealGreen">{{ $ctrl.maintitle }}</span>
        </div>
        <div class="actions dropdown inputs">
            <div class="inline-block" style="vertical-align: top; margin: 4px 0px; line-height: 26px;">
                <a class="btn green-haze btn-outline btn-circle btn-sm icon-wrapper" ng-class="$ctrl.enabledActionBtn ? '' : 'disable-links disabled'" href="javascript:;" data-toggle="dropdown" data-hover="dropdown" data-close-others="true" aria-expanded="true">
                    Actions
                    <i class="fa fa-angle-down"></i>
                </a>
                <ul class="dropdown-menu pull-right">
                    <li ng-repeat="model in $ctrl.action" ng-class="model.showBtn == true ? '' : 'disabled'">
                        <a ng-click="$ctrl.handleAction(model.action)" ng-class="model.showBtn == true ? '' : 'disable-links'">{{ $ctrl.L(model.action) }}</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="portlet-body">
        <div ng-if="$ctrl.raisoft">
            <div class="row" style="padding-bottom:10px">
                <div class="col-md-12">
                    <span class="fa fa-sticky-note-o" style="padding-right:5px"></span>
                    <span class="otherText">Assessment:</span>
                    <span class="bold ng-binding">{{$ctrl.raisoftPerson.assessment.description}}</span><br />
                    <span class="fa fa-calendar" style="padding-right:5px"></span>
                    <span class="otherText">Assessment Date:</span>
                    <span class="bold ng-binding">{{$ctrl.raisoftPerson.assessment.assessmentDate | dateFormat}}</span><br />
                    <span class="fa fa-lock" style="padding-right:5px"></span>
                    <span class="otherText">Assessment Locked Date:</span>
                    <span class="bold ng-binding">{{$ctrl.raisoftPerson.assessment.lockDate | dateFormat}}</span><br />
                </div>

            </div>
            <hr style="margin:15px 0" />
            <div>
                <a ng-click="$ctrl.scalesopen = !$ctrl.scalesopen" title="Show Scales" class="fontStealGreen">
                    <i ng-show="!$ctrl.scalesopen"class="glyphicon glyphicon-chevron-right"></i>
                    <i ng-show="$ctrl.scalesopen" class="glyphicon glyphicon-chevron-down"></i>
                </a>
                <span class="bold fontStealGreen">Scales</span>
            </div>
            <hr style="margin:15px 0" />
            <div ng-if="$ctrl.scalesopen">
                <div ng-repeat="data in $ctrl.scales">
                    <div class="row">
                        <div class="col-md-12 col-sm-12">
                            <span class="otherText bold">{{data.name}}</span>
                        </div>
                        <div class="col-md-12 col-sm-12">
                            <div class="col-md-7 col-sm-7" style="padding:0px">
                                <span class="otherText bold">{{data.result.valuesDescription}}</span>
                            </div>

                            <div class="col-md-4 col-sm-4">
                                <div class="otherText" uib-popover="Min: {{data.result.min}} - Max: {{data.result.max}}" popover-placement="bottom" popover-trigger="'mouseenter'">
                                    <table class="scalebar_container" ng-if="$ctrl.calculatePercent(data.result.min,data.result.Max,data.result.value) != 'empty'">
                                        <tr>
                                            <td class="min_value dgt3" ng-style="{color: $ctrl.calculatePercent(data.result.min,data.result.max,data.result.value) == 'negative' ? '#bf1f1f' : '#333333' }">
                                                {{data.result.min}}
                                            </td>
                                            <td>
                                                <div class="truncate" style="width: 150px;">


                                                    <div class="rai_scalebar none" style="height: 20px; position: relative; overflow: visible;" ng-if="$ctrl.calculatePercent(data.result.min,data.result.max,data.result.value) != 'negative'">
                                                        <div class="bar none" ng-style="{width: $ctrl.calculatePercent(data.result.min,data.result.max,data.result.value) }">&nbsp;</div>
                                                    </div>
                                                    <div class="rai_scalebar none" style="height: 20px; position: relative; overflow: visible;" disabled ng-disabled="true" ng-if="$ctrl.calculatePercent(data.result.min,data.result.max,data.result.value) == 'negative'">
                                                        <div class="bar none" ng-style="{width: $ctrl.calculatePercent(data.result.min,data.result.max,data.result.value) }">&nbsp;</div>
                                                    </div>
                                                </div>
                                            </td>
                                            <td class="max_value dgt3" style="text-align:left;" ng-style="{color: $ctrl.calculatePercent(data.result.min,data.result.max,data.result.value) == 'negative' ? '#bf1f1f' : '#333333' }">
                                                {{data.result.max}}
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>

                    </div>
                    <hr style="margin:15px 0" />
                </div>
            </div>
            <div>
                <a ng-click="$ctrl.capsopen = !$ctrl.capsopen" title="Show Caps" class="fontStealGreen">
                    <i ng-show="!$ctrl.capsopen" class="glyphicon glyphicon-chevron-right"></i>
                    <i ng-show="$ctrl.capsopen" class="glyphicon glyphicon-chevron-down"></i>
                </a>
                <span class="bold fontStealGreen">Caps</span>
            </div>
            <hr style="margin:15px 0" />
            <div ng-if="$ctrl.capsopen">
                <div ng-repeat="data in $ctrl.caps">
                    <div class="row">
                        <div class="col-md-12 col-sm-12">
                            <span class=" fa fa-exclamation-circle text-danger"></span>
                            <span class="otherText bold">{{data.description}}</span>
                        </div>
                        <div class="col-md-12 col-sm-12">
                            <span class="otherText bold">{{data.triggered}}</span>
                        </div>
                    </div>
                    <hr style="margin:15px 0" />
                </div>
            </div>

            <div>
                <a ng-click="$ctrl.rugopen = !$ctrl.rugopen" title="Show Rug" class="fontStealGreen">
                    <i ng-show="!$ctrl.rugopen" class="glyphicon glyphicon-chevron-right"></i>
                    <i ng-show="$ctrl.rugopen" class="glyphicon glyphicon-chevron-down"></i>
                </a>
                <span class="bold fontStealGreen">Rug</span>
            </div>
            <hr style="margin:15px 0" />
            <div ng-if="$ctrl.rugopen">
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <span class="otherText bold">{{$ctrl.rug.group_longname}}</span>
                    </div>
                    <div class="col-md-12 col-sm-12">
                        <span class="otherText bold">{{$ctrl.rug.group}} - {{$ctrl.rug.group_desc}}</span>
                    </div>
                </div>
                <hr style="margin:15px 0" />
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <span class="otherText bold">{{$ctrl.rug.cmi_longname}}</span>
                    </div>
                    <div class="col-md-12 col-sm-12">
                        <span class="otherText bold">{{$ctrl.rug.cmi}}</span>
                    </div>
                </div>
                <hr style="margin:15px 0" />
                <div class="row">
                    <div class="col-md-12 col-sm-12">
                        <span class="otherText bold">{{$ctrl.rug.maingroup_longname}}</span>
                    </div>
                    <div class="col-md-12 col-sm-12">
                        <span class="otherText bold">{{$ctrl.rug.maingroup_value}} - {{$ctrl.rug.maingroup}}</span>
                    </div>
                </div>
                <hr style="margin:15px 0" />
            </div>

        </div>

        <div class="row" ng-if="!$ctrl.raisoft">
            <div class="col-md-12  col-sm-12">
                <span ng-if="$ctrl.filter == 'active'"></span>
                <span>No records found!</span>
            </div>
        </div>
    </div>

</div>



